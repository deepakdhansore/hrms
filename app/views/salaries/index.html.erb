<%= render "employees/header"%>
 
<h1>Salary Status</h1>
<table>
<th> <strong>Start Date</strong></th>
 <th> <strong>End Date</strong></th>
 <th><strong>Salary</strong></th>
 
 
<% @salary.each do |salary|%>
 
 <tr>
   <td> <%= salary.fromdate.strftime("%d/%m/%Y") %></td>
   <td><%= salary.todate.strftime("%d/%m/%Y") %></td>
  <td><%= salary.salary %></td>
  
 </tr>
<% end %>
</table>